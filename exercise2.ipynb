    "def create_tuples(list1, list2):\n",
    "    return [(list1[i], list2[i]) for i in range(min(len(list1), len(list2)))]\n",
    "\n",
    "list1 = [1, 2, 3,]\n",
    "list2 = [\"mark\", \"alice\", \"john\"]\n",
    "\n",
    "result = create_tuples(list1, list2)\n",
    "print(result)"
   ]
